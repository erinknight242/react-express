{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "jsx-a11y",
    "react"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [0, "always-multiline"],
    "eqeqeq": [2, "smart"],
    "func-names": 0,
    "global-require": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-quotes": [2, "prefer-double"],
    "linebreak-style": 0,
    "new-cap": 0,
    "no-else-return": 0,
    "no-multi-spaces": 0,
    "quotes": [2, "single"],
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-no-bind": 0,
    "react/no-array-index-key": 0,
    "react/no-did-mount-set-state": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 1,
    "react/sort-comp": 0
  }
}
